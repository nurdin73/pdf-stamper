{
  "name": "karungbolong/pdf-stamper",
  "description": "Laravel PDF stamping and watermarking with TCPDF & FPDI",
  "type": "library",
  "require": {
    "php": "^8.0",
    "tecnickcom/tcpdf": "^6.7",
    "setasign/fpdi": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0|^11.0",
    "orchestra/testbench": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "KarungBolong\\PdfStamper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KarungBolong\\PdfStamper\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "KarungBolong\\PdfStamper\\PdfStamperServiceProvider"
      ],
      "aliases": {
        "PdfStamper": "KarungBolong\\PdfStamper\\Facades\\PdfStamper"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
